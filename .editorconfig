root = true

[*]
indent_style = tab
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.js]
indent_size = 4

[*.ts]
indent_size = 4

[*.vue]
indent_size = 4

[*.css]
indent_size = 4

[*.scss]
indent_size = 4

[Makefile]
indent_style = tab
